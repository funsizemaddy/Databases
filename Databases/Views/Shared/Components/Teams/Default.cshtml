

@model IEnumerable<string>

<div class="d-grid gap-2">

    <a asp-controller="Home" asp-action="Index" asp-route-TeamName="" class="btn btn-outline-secondary">Home</a><br />
    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-TeamName="@t" class=" btn @(ViewBag.SelectedType == t ? "btn-primary" : " btn btn-outline-secondary")">@t</a>
        <br />
    }

</div>
